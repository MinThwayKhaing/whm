	color: #ffffff;
}

.whmFooterIcon {
	margin-top: -4px;
	margin-left: 340px;
	position: absolute;
	float: right;
}

.whmFooter {
	font-family: segoe ui, Arial, sans-serif;
	font-size: 12px;
	margin-right: 530px;
	float: right;
}

.whmLayoutFooter .ui-layout-unit .ui-layout-unit-content {
	overflow: hidden;
}

.whmError {
	font-family: segoe ui, Arial, sans-serif;
	float: left;
	position: absolute;
	margin-top: -400px;
	margin-left: 600px;
